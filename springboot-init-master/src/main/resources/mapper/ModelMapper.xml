<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.louis.springbootinit.mapper.ModelMapper">

    <resultMap id="BaseResultMap" type="com.louis.springbootinit.model.entity.Model">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="modelid" column="modelId" jdbcType="VARCHAR"/>
            <result property="modelname" column="modelName" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,modelId,modelName
    </sql>
</mapper>
